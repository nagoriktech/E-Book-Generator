    {
      "parameters": {
        "jsCode": "var Bookcontent = $input.first().json['Chapter Content'] || [];\n\n// 1. Get length of the content\nvar len = Bookcontent.length;\n\n// 2–3. Loop and merge all string items\nvar mergedContent = '';\nfor (var i = 0; i < len; i++) {\n  mergedContent += Bookcontent[i] + '\\n';\n}\n\n// 4. Wrap merged content with required multipart structure\nvar finalOutput =\n`--divider\\r\nContent-Type: application/json; charset=UTF-8\\r\n\\r\n{\"mimeType\":\"application/vnd.google-apps.document\"}\\r\n--divider\\r\nContent-Type: text/html\\r\n\\r\n<!DOCTYPE html>\n<html>\n<head>\n  <meta charset=\"UTF-8\">\n  <style>\n    p, ul, ol, table, h1, h2, h3, h4, h5, h6 {\n      margin-bottom: 10pt;\n    }\n    h2 {\n      margin-top: 20pt;\n    }\n    li {\n      margin-bottom: 2pt;\n    }\n    a {\n      color: #1155cc;\n      text-decoration: underline;\n    }\n  </style>\n</head>\n<body>\n${mergedContent}\n</body>\n</html>\n\\r\n--divider--\\r\n`;\n\n// 5. Return final output\nreturn {\n  json: {\n    output: finalOutput\n  }\n};\n"
      },
